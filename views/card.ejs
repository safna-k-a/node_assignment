<%-include("header")%>
<div class="container mt-5">
    <div class="row">
        <% products.forEach(product => { %>
        <div class="col-md-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"><%= product.name %></h5>
                    <p class="card-text">$<%= product.price %></p>
                    <a href="<%= product.url %>" class="btn btn-primary"
                        >Buy Now</a
                    >
                </div>
            </div>
        </div>
        <% }); %>
    </div>
</div>
<%-include("footer")%>
